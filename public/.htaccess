<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirect brycerambach.com -> brycedigital.io
  RewriteCond %{HTTP_HOST} ^brycerambach\.com$ [NC,OR]
  RewriteCond %{HTTP_HOST} ^www\.brycerambach\.com$ [NC]
  RewriteRule ^(.*)$ https://brycedigital.io/$1 [R=301,L]

  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule . /index.html [L]
</IfModule>
